  "names": [
    {
      "language": "fi",
      "value": "Yleiskaavoitukseen vaikuttaminen",
      "type": "Name"
    }
  ],
  "descriptions": [
    {
      "language": "fi",
      "value": "Yleiskaava kertoo, miten kunta suunnittelee alueiden käyttöä ja jakamista asumisen, työelämän, liikenteen, virkistyksen ym. kesken.",
      "type": "ShortDescription"
    },
    {
      "language": "fi",
      "value": "Kaikilla, joiden oloihin tai etuihin yleiskaavoitus vaikuttaa, on oikeus osallistua yleiskaavoitusprosessiin ja vaikuttaa siihen osaltaan.\n\nYleiskaava kertoo, miten kunta suunnittelee alueidensa käyttöä ja jakamista asumisen, työelämän, liikenteen, virkistyksen ja muiden toimintojen kesken. Yleiskaavasta säädetään maankäyttö- ja rakennuslaissa.\n\nKunta voi laatia yleiskaavan joko yksin tai yhdessä naapurikuntiensa (yhteinen yleiskaava) kanssa.",
      "type": "Description"
    }
  ],
